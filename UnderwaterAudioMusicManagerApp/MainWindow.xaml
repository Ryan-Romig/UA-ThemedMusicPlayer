                        </Button>
                        <!--shuffle button-->
                        <Button Style="{DynamicResource shuffleToggleOnStyle}"  x:Name="shuffleButtonOn" Width="24px" BorderBrush="{x:Null}" Foreground="{x:Null}" Click="shuffleButton_Click" Visibility="Collapsed" >
                            <Button.Effect>
                                <DropShadowEffect/>
                            </Button.Effect>
                            <Button.OpacityMask>
                                <ImageBrush ImageSource="/icons8-shuffle-64.png" Stretch="Uniform"/>
                            </Button.OpacityMask>
                        </Button>

                    </StackPanel>

                </Grid>


                <!--END CENTER CONTROLS-->

                <!-- Start Volume Controls-->
                <Grid Grid.Row="0" Grid.Column="4">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>

                    <DockPanel Grid.Column="1"  >
                        <Slider Style="{DynamicResource TrackSliderStyle}" MaxWidth="125px" VerticalAlignment="Center" x:Name="volumeSlider"
                            Margin="0,0,0,0"
                            Background="{x:Null}"
                            DockPanel.Dock="Left"
                            IsSnapToTickEnabled="False"
                            LargeChange="5"
                            Maximum="100"
                            Minimum="0"
                            Orientation="Horizontal"
                            SmallChange="1"
                            TickFrequency="1"
                            Value="100" BorderBrush="{x:Null}" Foreground="#FFFF0F0F" ValueChanged="volumeSlider_Change" AutoToolTipPlacement="TopLeft" Cursor="SizeWE" Opacity="0.8" >
                            <Slider.Effect>
                                <DropShadowEffect/>
                            </Slider.Effect>
                        </Slider>
                    </DockPanel>
                </Grid>
                <ProgressBar  x:Name="trackProgressBar" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" BorderBrush="{x:Null}" Margin="0,24,0,25" Opacity="0.35" Foreground="#FFF8971D" Background="{x:Null}">
                    <ProgressBar.Effect>
                        <BlurEffect/>
                    </ProgressBar.Effect>
                </ProgressBar>
                <Slider Style="{DynamicResource TrackSliderStyle}" LargeChange="5" Height="22px" Maximum="100"  x:Name="trackProgressSlider" VerticalAlignment="Center" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" ValueChanged="trackProgressSlider_Change" Opacity="0.8" AutoToolTipPlacement="TopLeft" SmallChange="1" Cursor="ScrollWE" />
                <TextBlock x:Name="trackTimeElapsedTextBox" Grid.Row="1" Grid.Column="1" HorizontalAlignment="left" Margin="0,29,10,0" Text="0:00" VerticalAlignment="Bottom"  Width="auto" FontSize="18" FontStretch="Condensed" TextWrapping="WrapWithOverflow" FontWeight="Normal" FontFamily="Segoe UI" >
                    <TextBlock.Effect>
                        <DropShadowEffect/>
                    </TextBlock.Effect></TextBlock>
                <TextBlock  x:Name="trackDurationTextBox" Grid.Row="1" Grid.Column="3" HorizontalAlignment="Right" Text="0:00" VerticalAlignment="Bottom" Margin="20,0,0,0"  Width="auto" FontSize="18" TextWrapping="WrapWithOverflow" FontStretch="UltraExpanded" FontWeight="Light" FontFamily="Segoe UI" >
                    <TextBlock.Effect>
                        <DropShadowEffect/>
                    </TextBlock.Effect></TextBlock>







            </Grid>
        </Grid>






    </Grid>

</Window>
